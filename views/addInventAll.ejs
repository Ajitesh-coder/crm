        <h1>Add Inventory</h1>

        <div class="fillexist">
          <form action="/addinginventory" class="filldata" method="post"> 
            <div class="topfill">
              <input type="checkbox" name="check" id="check1"/>
              <label for="Number">Property Number</label>
              <label for="Address">Address</label>
              <label for="SubCategory">SubCategory</label>
              <label for="Size">Size</label>
              <label for="subloc">Sub Location</label>
              <label for="width">Width </label>
              <label for="len">Length</label>
              <label for="metrics">Plot Area</label>
              <label for="Direction">Direction</label>
              <label for="facing">Facing</label>
              <label for="road">Road</label>
              <label for="nearto">Near To</label>
              <label for="ownership">Ownership</label>
              <label for="location">Location</label>
              <label for="detailing">Owner Details</label>
            </div>

            <div class="menufill" id="addNew">
              <input type="checkbox" name="check" id="check2" />
              <input type="number" name="number" id="inNumber" min="0" />
              <input type="text" name="address" id="Address">
              <select id="subCategory" name="subCategory">
                <option value="Shop">Shop</option>
                <option value="Showroom">Showroom</option>
                <option value="Office">Office</option>
                <option value="RetailStore">RetailStore</option>
                <option value="HyperMarket">HyperMarket</option>
                <option value="SOHO">SOHO</option>
                <option value="Multiplex">Multiplex</option>
              </select>
              <select name="size" id="size">
                <option value="SCO">SCO</option>
                <option value="SCF">SCF</option>
                <option value="Booth">Booth</option>
                <option value="Dss">Dss</option>
                <option value="Keosak">Keosak</option>
                <option value="Plot">Plot</option>
              </select>
              <select name="subLoc" id="subloc">
                <option value="A">A</option>
                <option value="B">B</option>
                <option value="C">C</option>
                <option value="D">D</option>
              </select>
              <input type="number" name="width" id="width" min="0" />
              <input type="number" name="len" id="len" min="0" />
              <!-- <input type="number" name="" id=""> -->
              <select name="metrics" id="metrics">
                <option value="Metre">Metrics</option>
                <option value="sqaure yard">Sq Yard</option>
                <option value="acre">Acre</option>
                <option value="kanal">Kanal</option>
              </select>
              <select name="direction" id="direction">
                <option value="north">North</option>
                <option value="south">South</option>
                <option value="east">East</option>
                <option value="northeast">NorthEast</option>
                <option value="southeast">SouthEast</option>
                <option value="northwest">NorthWest</option>
                <option value="southwest">SouthWest</option>
              </select>
              <select name="facing" id="facing">
                <option value="Yes">Parking</option>
                <option value="No">Dead Face</option>
              </select>
              <select name="road" id="road">
                <option value="9 Meter">9Mtr</option>
                <option value="12 Meter">12Mtr</option>
                <option value="18 Meter">18Mtr</option>
                <option value="34 Meter">24Mtr</option>
                <option value="60 Meter">60Mtr</option>
              </select>
              <select name="near" id="nearto">
                <option value="school">School</option>
                <option value="Station">Station</option>
                <option value="BusStand">Bus Stand</option>
                <option value="MainRoad">Main Road</option>
                <option value="Highway">HighWay</option>
              </select>
              <label for="ownership">Ownership</label>
              <select name="ownership" id="ownership">
                <option value="Freehold">Freehold</option>
                <option value="Leasehold">LeaseHold</option>
              </select>

              <button class="btn" id="location">
                Location
              </button>
              <button class="btn">
                Owner Details
              </button>
            </div>
            <button id="btn" type="submit">Submit Inventory</button>
          </form>
        </div>
      </div>

